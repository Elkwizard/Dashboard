{
    "layoutName": "Test2019.json",
    "pageTemplates": [
        {
            "icon": "fas fa-gamepad primary",
            "title": "Compete",
            "widgets": [
                {
                    "id": "Compete",
                    "label": "Driver Main Interface",
                    "type": "systemstate",
                    "size": ["fill", 40]
                },
                {
                    "title": "Field Configuration",
                    "id": "fieldconfig",
                    "type": "fieldconfig",
                    "size": [400, 490],
                    "ntkeys": [
                        "/SmartDashboard/AutoStrategy",
                        "/SmartDashboard/AutoStrategyOptions",
                        "/FMSInfo/IsRedAlliance",
                        "/FMSInfo/StationNumber",
                        "/FMSInfo/GameSpecificMessage",
                        "/FMSInfo/ReplayNumber",
                        "/FMSInfo/FMSControlData",
                        "/FMSInfo/EventName",
                        "/FMSInfo/MatchType",
                        "/FMSInfo/MatchNumber"
                    ],
                    "params": {

                    }
                },
                {
                    "title": "Cameras",
                    "id": "cameras",
                    "type": "cameras",
                    "size": [650, 650],
                    "ntkeys": [
                        "/SmartDashboard/CameraView"
                    ],
                    "params": {
                        "CubeCam": 
                        {
                            "_notes": "dlink13",
                            "ip": "admin:@10.49.15.13",
                            "url": "/video.cgi",
                            "cls": "rotate90"
                        },
                        "LiftCam": 
                        {
                            "_notes": "dlink14",
                            "ip": "admin:@10.49.15.14",
                            "url": "/cam/mjpg",
                            "cls": "rotate0"
                        },
                        "PiCam": 
                        {
                            "ip": "10.49.15.10:5080",
                            "url": "/cam/mjpg",
                            "cls": "rotate0"
                        }
                    }
                }
            ]
        },
        {
            "title": "Drive",
            "icon":  "fas fa-tachometer-alt",
            "widgets": [
                {
                    "id": "Drive",
                    "label": "Drive Subsystem",
                    "type": "systemstate",
                    "size": ["fill", 40],
                    "ntkeys": ["/SmartDashboard/Drive/State",
                              "/SmartDashboard/Drive/Status"]
                },
                {
                    "id": "driveSpeedLeft",
                    "label": "Left Speed",
                    "type": "stripchart",
                    "size": [330, 170],
                    "ntkeys": "/SmartDashboard/Drive/leftSpeed",
                    "ntkeyRefs":[
                        "/SmartDashboard/Drive/leftSpeedTarget"
                    ],
                    "params": {
                        "plot": {
                            "yaxis": {
                                "min": 0, 
                                "max": 80
                            },
                            "fillvalue": 0,
                            "colors":["rgb(20,120,255)", "rgb(200,200,10)"],
                            "channelcount": 2,
                            "widths": [2, 1],
                            "maxlength": 400
                        }
                    }
                },
                {
                    "id": "driveSpeedRight",
                    "label": "Right Speed",
                    "type": "stripchart",
                    "size": [330, 170],
                    "ntkeys": "/SmartDashboard/Drive/rightSpeed",
                    "ntkeyRefs":[
                        "/SmartDashboard/Drive/rightSpeedTarget"
                    ],
                    "params": {
                        "plot": {
                            "yaxis": {
                                "min": 0, 
                                "max": 80
                            },
                            "fillvalue": 0,
                            "colors":["rgb(20,120,255)", "rgb(200,200,10)"],
                            "channelcount": 2,
                            "widths": [2, 1],
                            "maxlength": 400
                        }
                    }
                },
                {
                    "id": "driveIMU",
                    "label": "IMU Heading",
                    "type": "stripchart",
                    "size": [330, 170],
                    "ntkeys": "/SmartDashboard/Drive/imuHeading",
                    "params": {
                        "plot": {
                            "yaxis": {
                                "min": -200, 
                                "max": 200
                            },
                            "widths": [2],
                            "colors":["rgb(20,120,255)"]
                        }
                    }
                },
                {
                    "id": "driveOdometry",
                    "label": "Odometry",
                    "type": "pathplot",
                    "size": [330, 170],
                    "ntkeys": "/SmartDashboard/Drive/odometry",
                    "params": {
                        "plot": {
                            "xaxis": {
                                "min": 0,
                                "max": 652,
                                "show": 1
                            },
                            "yaxis": {
                                "min": 0,
                                "max": 324,
                                "show": 1
                            },
                            "series": {
                                "shadowSize": 0,
                                "lines": {
                                    "show": 1,
                                    "lineWidth": 1
                                },
                                "points": {
                                    "show": 0
                                },
                                "color": "rgb(20, 120, 255)"
                            }
                        }
                    }
                },
                {
                    "id": "Lidar",
                    "label": "Lidar",
                    "type": "systemstate",
                    "size": ["fill", 40],
                    "ntkeys": ["/SmartDashboard/Lidar/status",
                               "/SmartDashboard/Lidar/angle",
                               "/SmartDashboard/Turret/state"
                            ]
                },
                {
                    "id": "lidarOdometry",
                    "label": "LIDAR Odometry",
                    "type": "pathplot",
                    "size": [330, 170],
                    "ntkeys": "/SmartDashboard/Lidar/pose",
                    "params": {
                        "plot": {
                            "xaxis": {
                                "min": 0,
                                "max": 652,
                                "show": 1
                            },
                            "yaxis": {
                                "min": 0,
                                "max": 324,
                                "show": 1
                            },
                            "series": {
                                "shadowSize": 0,
                                "lines": {
                                    "show": 1,
                                    "lineWidth": 1
                                },
                                "points": {
                                    "show": 0
                                },
                                "color": "rgb(20, 120, 255)"
                            }
                        }
                    }
                },
                {
                    "id": "lidarPoints",
                    "label": "LIDAR Points",
                    "type": "pathplot",
                    "size": [330, 170],
                    "ntkeys": "/SmartDashboard/Lidar/points",
                    "params": {
                        "plot": {
                            "xaxis": {
                                "min": 0,
                                "max": 652,
                                "show": 1
                            },
                            "yaxis": {
                                "min": 0,
                                "max": 324,
                                "show": 1
                            },
                            "series": {
                                "shadowSize": 0,
                                "lines": {
                                    "show": 0
                                },
                                "points": {
                                    "show": 1,
                                    "radius": 0.05,
                                    "fill": 0
                                },
                                "color": "rgb(20, 120, 255)"
                            },
                            "maxlength":  1e4
                        }
                    }
                }
            ]
        },
        {
            "title": "Climb",
            "icon":  "fas fa-rocket",
            "widgets": [
                {
                    "id": "Climb",
                    "label": "Climb Subsystem",
                    "type": "systemstate",
                    "size": ["fill", 40],
                    "ntkeys": [
                        "/SmartDashboard/Climb/State",
                        "/SmartDashboard/Climb/Status"
                    ]
                }
            ]
        },
        {
            "title": "Vision",
            "icon":  "fas fa-eye",
            "widgets": [
                {
                    "id": "vision",
                    "label": "Vision Subsystem",
                    "type": "systemstate",
                    "size": ["fill", 40],
                    "ntkeys": "/SmartDashboard/Vision/State"
                }
            ]
        },
        {
            "title": "Test",
            "icon": "fas fa-vial",
            "widgets": [
