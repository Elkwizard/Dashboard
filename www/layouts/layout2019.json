{
    "layoutName": "Test2019.json",
    "pageTemplates": [
        {
            "icon": "fas fa-gamepad primary",
            "title": "Compete",
            "widgets": [
            ]
        },
        {
            "title": "Drive",
            "icon":  "fas fa-tachometer-alt",
            "widgets": [
                {
                    "id": "drive",
                    "label": "Drive Subsystem",
                    "type": "systemstate",
                    "size": ["fill", 40],
                    "ntkey": ["/SmartDashboard/Drive/State",
                              "/SmartDashbaord/Drive/Status"]
                },
                {
                    "id": "driveSpeedLeft",
                    "label": "Left Speed",
                    "type": "stripchart",
                    "size": [270, 160],
                    "ntkey": "/SmartDashboard/Drive/leftSpeed",
                    "ntkeyRefs":[
                        "/SmartDashboard/Drive/leftSpeedTarget"
                    ],
                    "params": {
                        "yaxis": {
                            "min": 0, 
                            "max": 80
                        },
                        "fillvalue": 0,
                        "colors":["rgb(20,120,255)", "rgb(200,200,10)"],
                        "channelcount": 2,
                        "widths": [2, 1],
                        "maxlength": 400
                    }
                },
                {
                    "id": "driveSpeedRight",
                    "label": "Right Speed",
                    "type": "stripchart",
                    "size": [270, 160],
                    "ntkey": "/SmartDashboard/Drive/rightSpeed",
                    "ntkeyRefs":[
                        "/SmartDashboard/Drive/rightSpeedTarget"
                    ],
                    "params": {
                        "yaxis": {
                            "min": 0, 
                            "max": 80
                        },
                        "fillvalue": 0,
                        "colors":["rgb(20,120,255)", "rgb(200,200,10)"],
                        "channelcount": 2,
                        "widths": [2, 1],
                        "maxlength": 400
                    }
                },
                {
                    "id": "driveIMU",
                    "label": "IMU Heading",
                    "type": "stripchart",
                    "size": [270, 160],
                    "ntkey": "/SmartDashboard/Drive/imuHeading",
                    "params": {
                        "yaxis": {
                            "min": -200, 
                            "max": 200
                        },
                        "widths": [1],
                        "colors":["rgb(20,120,255)"]
                    }
                },
                {
                    "id": "driveOdometry",
                    "label": "Odometry",
                    "type": "pathplot",
                    "size": [270, 160],
                    "ntkey": "/SmartDashboard/Drive/odometry",
                    "params": {
                        "xaxis": {
                            "min": 0,
                            "max": 652,
                            "show": 1
                        },
                        "yaxis": {
                            "min": 0,
                            "max": 324,
                            "show": 1
                        },
                        "series": {
                            "shadowSize": 0,
                            "lines": {
                                "show": 1,
                                "lineWidth": 1
                            },
                            "points": {
                                "show": 0
                            },
                            "color": "rgb(20, 120, 255)"
                        }
                    }
                }
            ]
        },
        {
            "title": "Climb",
            "icon":  "fas fa-rocket",
            "widgets": [
                {
                    "id": "climb",
                    "label": "Climb Subsystem",
                    "type": "systemstate",
                    "size": ["fill", 40],
                    "ntkey": "/SmartDashboard/Climb"
                }
            ]
        },
        {
            "title": "Vision",
            "icon":  "fas fa-eye",
            "widgets": [
                {
                    "id": "vision",
                    "label": "Vision Subsystem",
                    "type": "systemstate",
                    "size": ["fill", 40],
                    "ntkey": "/SmartDashboard/Vision"
                }
            ]
        },
        {
            "title": "Test",
            "icon": "fas fa-vial",
            "widgets": [
            ]
        },
        {
            "title": "Network Tables",
            "icon": "fas fa-network-wired",
            "pagehandler": "NetTabPH",
            "widgets": [
                {
                    "id": "nettab",
                    "type": "html",
                    "size": ["fill", "fill"],
                    "params": {
                        "url": "/pages/nettab.html"
                    }
                }
            ]
        },
        {
            "title": "Robot Log",
            "icon":  "fas fa-robot",
            "pagehandler": "RobotlogPH",
            "widgets": [
                {
                    "id": "robolog",
                    "type": "html",
                    "size": ["fill", "fill"],
                    "params": {
                        "url": "/pages/robotlog.html"
                    }
                }
            ]
        },
        {
            "title": "About",
            "icon": "fas fa-cogs",
            "widgets": [
                {
                    "id": "about",
                    "type": "html",
                    "size": [400, 280],
                    "params": {
                        "url": "/pages/about.html"
                    }
                },
                {
                    "id": "about2",
                    "type": "html",
                    "size": [400, 280],
                    "params": {
                        "url": "/pages/about.html"
                    }
                },
                {
                    "id": "about3",
                    "type": "html",
                    "size": [400, 280],
                    "params": {
                        "url": "/pages/about.html"
                    }
                },
                {
                    "id": "about4",
                    "type": "html",
                    "size": [400, 280],
                    "params": {
                        "url": "/pages/about.html"
                    }
                }
            ]
        }
    ]
}