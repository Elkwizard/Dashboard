        { "_comment_page": "--------- test page -------------------------- ",
            "navtext": "Test",
            "icon": "fas fa-vial",
            "widgets": [
                {
                    "type": "systemstate",
                    "id": "Test",
                    "label": "Test Subsystem",
                    "size": ["fill", "row"],
                    "ntkeys": []
                },
                {
                    "type": "selector",
                    "label": "Test Mode",
                    "id": "testmode",
                    "size": [220, "row"],
                    "ntkeys": ["TestMode",
                               "TestModeOptions"],
                    "params": {
                        "width": "9em",
                        "ntkey": "TestMode",
