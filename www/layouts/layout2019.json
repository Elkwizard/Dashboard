            "widgets": [
                {
                    "id": "nettab",
                    "type": "nettab",
                    "size": ["fill", "fill"]
                }
            ]
        },
        {
            "title": "Robot Log",
            "icon":  "fas fa-robot",
            "pagehandler": "RobotlogPH",
            "widgets": [
                {
                    "id": "robotlog",
                    "type": "robotlog",
                    "size": ["fill", "fill"]
                }
            ]
        },
        {
            "title": "About",
            "icon": "fas fa-cogs",
            "widgets": [
